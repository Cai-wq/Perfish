{
  "name": "perf-fish",
  "version": "1.0.0",
  "author": "蔡伟琪 <caiweiqi@lizhi.fm>",
  "description": "移动端性能测试工具",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "export CSC_IDENTITY_AUTO_DISCOVERY=false && node .electron-vue/build.js && electron-builder",
    "build:dir": "export CSC_IDENTITY_AUTO_DISCOVERY=false && node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "rebuild": "./node_modules/.bin/electron-rebuild && npm install",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "mac": "export CSC_IDENTITY_AUTO_DISCOVERY=false && node .electron-vue/build.js && electron-builder build --mac",
    "pack-app": "npm run pack:main && npm run pack:renderer && ./node_modules/.bin/electron-packager . --overwrite --ignore=py$",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "PerfFish",
    "appId": "fm.lizhi.testing.perfish",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/gaff.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "0.18.1",
    "echarts": "4.2.1",
    "electron-log": "^4.4.1",
    "element-ui": "2.8.2",
    "fix-path": "^4.0.0",
    "js-cookie": "^2.2.0",
    "node-cmd": "^5.0.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "vue": "2.6.10",
    "vue-electron": "^1.0.6",
    "vue-router": "3.0.6",
    "vuex": "3.1.0",
    "vuex-persist": "^3.1.3",
    "zerorpc": "^0.9.7"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "6.0.0",
    "electron-builder": "21.2.0",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-log": "^4.4.1",
    "electron-rebuild": "^3.2.2",
    "eslint": "^4.4.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "sass-loader": "6.0.6",
    "style-loader": "^0.18.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "2.6.10",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
